            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="When the clone of the rocket is created;"/>
                <para PARENT="Paragraph"/>
                <ITEXT CH="Put it at the bottom of the screen underneath the mouse."/>
                <para PARENT="Bullet"/>
                <ITEXT CH="Make it fly up to the mouse with a &quot;whoosh!&quot;"/>
                <para PARENT="Bullet"/>
